<%- include("header") -%>
    <div class="box" id="heading">
        <h1><%=kindOfDay%></h1>
    </div>
    <div class="box">

        <%for (var i = 0; i < items.length; i++){%>
        <div class="item">
            <input type="checkbox">
            <p><%=items[i]%></p>
        </div>
        <%}%>
            <form method="post" action="/">
            <input type="text" name="newItem" autocomplete="off">
            <button type="submit" name="btn" value = <%= kindOfDay %>>+</button>
        </form>
    </div>
<%- include("footer") -%>